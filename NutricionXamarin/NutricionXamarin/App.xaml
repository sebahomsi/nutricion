<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="NutricionXamarin.App"    
             xmlns:infra ="clr-namespace:NutricionXamarin.Infraestructura;assembly=NutricionXamarin">
    <Application.Resources>
        <ResourceDictionary>
            <infra:InstanceLocator x:Key="Locator"/>
            <Color x:Key="MainColor">#89bc94</Color>
            <Color x:Key="FontColor">#000000</Color>
            <Color x:Key="FontColor1">#FFFFFF</Color>
            <Color x:Key="AccentColor1">#364a3b</Color>
            <Color x:Key="AccentColor2">#b8b89f</Color>
            <Color x:Key="BackgroundColor">#66a273</Color>
        </ResourceDictionary>
    </Application.Resources>
</Application>