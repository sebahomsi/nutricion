@model IEnumerable<NutricionWeb.Models.Paciente.PacienteViewModel>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ApyNom)
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>

        <td>
            @Html.DisplayFor(modelItem => item.ApyNom)
        </td>
        
        <td>
            <input type="button" id="seleccionar-paciente" name="@item.Id" class="btn btn-primary-custom" value="Seleccionar" />

        </td>
    </tr>
}

</table>
