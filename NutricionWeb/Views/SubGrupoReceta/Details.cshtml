}


<div class="titulo-ABM">
    <h4 class="detail-h4">SubGrupos de Recetas</h4>
</div>

<div class="row">
    <hr />
    <div class="col-md-7">
        <p class="info-style">
            @Html.DisplayNameFor(model => model.GrupoRecetaStr):
            <span class="sub-info">

                @Html.DisplayFor(model => model.GrupoRecetaStr)
            </span>

        </p>

        <p class="info-style">
            @Html.DisplayNameFor(model => model.Descripcion):
